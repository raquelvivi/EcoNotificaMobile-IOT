{
  "expo": {
    "name": "Front",
    "slug": "Front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icone_reciclagem.png",
    "scheme": "front",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icone_reciclagem.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.welsonrodrigues.Front"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icone_reciclagem.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icone_reciclagem.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "74fa3c2a-be96-48e8-b744-d64fb7627367"
      }
    }
  }
}

